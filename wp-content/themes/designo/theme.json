{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1110px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Jost",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/jost/jost-500-normal.woff2"
							]
						},
						{
							"fontFamily": "Jost",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/jost/jost-400-normal.woff2"
							]
						}
					],
					"fontFamily": "Jost, sans-serif",
					"name": "Jost",
					"slug": "jost"
				}
			],
			"fontSizes": [
				{
					"name": "Body",
					"size": "16px",
					"slug": "custom-1"
				},
				{
					"name": "Medium",
					"size": "20px",
					"slug": "custom-2"
				},
				{
					"fluid": {
						"max": "40px",
						"min": "28px"
					},
					"name": "Large",
					"size": "40px",
					"slug": "custom-3"
				},
				{
					"fluid": {
						"max": "48px",
						"min": "32px"
					},
					"name": "Extra Large",
					"size": "48px",
					"slug": "custom-4"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--custom-4)",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--custom-3)",
					"letterSpacing": "2px",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--custom-2)",
					"letterSpacing": "5px",
					"lineHeight": "1.3"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "24px",
				"right": "24px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--jost)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}